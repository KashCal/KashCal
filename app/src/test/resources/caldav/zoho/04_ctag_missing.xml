<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!-- Zoho PROPFIND response when getctag is requested on a path that doesn't support it.
     Captured pattern from calendar.zoho.com (2026-02-12).

     Note: Live testing (2026-02-12) showed Zoho DOES support getctag on the
     calendar collection (/caldav/{hash}/events/). This fixture models the case
     where a server returns 404 for getctag â€” used to test KashCal's graceful
     handling of servers that don't support this CalendarServer extension.
-->
<D:multistatus xmlns:D="DAV:" xmlns:cs="http://calendarserver.org/ns/">
  <D:response>
    <D:href>/caldav/a1b2c3d4e5f6a1b2c3d4e5f6a1b2c3d4/events/</D:href>
    <D:propstat>
      <D:prop>
        <D:displayname>My Calendar</D:displayname>
      </D:prop>
      <D:status>HTTP/1.1 200 OK</D:status>
    </D:propstat>
    <!-- getctag requested but returned as 404 (not found) -->
    <D:propstat>
      <D:prop>
        <cs:getctag/>
      </D:prop>
      <D:status>HTTP/1.1 404 Not Found</D:status>
    </D:propstat>
  </D:response>
</D:multistatus>