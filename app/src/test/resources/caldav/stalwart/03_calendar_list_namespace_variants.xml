<?xml version="1.0" encoding="UTF-8"?>
<!-- Test different namespace prefixes for calendar element -->
<!-- Servers may use A:calendar (Apple), C:calendar (CalDAV), or bare calendar -->
<D:multistatus xmlns:D="DAV:" xmlns:A="http://apple.com/ns/ical/" xmlns:cs="http://calendarserver.org/ns/" xmlns:C="urn:ietf:params:xml:ns:caldav">
    <!-- Calendar using C: prefix (CalDAV namespace) -->
    <D:response>
        <D:href>/dav/cal/admin/caldav-prefix-calendar/</D:href>
        <D:propstat>
            <D:prop>
                <D:resourcetype>
                    <D:collection/>
                    <C:calendar/>
                </D:resourcetype>
                <D:displayname>CalDAV Prefix Calendar</D:displayname>
                <cs:getctag>ctag-caldav-prefix</cs:getctag>
            </D:prop>
            <D:status>HTTP/1.1 200 OK</D:status>
        </D:propstat>
    </D:response>
    <!-- Calendar using A: prefix (Apple namespace) - common in iCloud responses -->
    <D:response>
        <D:href>/dav/cal/admin/apple-prefix-calendar/</D:href>
        <D:propstat>
            <D:prop>
                <D:resourcetype>
                    <D:collection/>
                    <A:calendar/>
                </D:resourcetype>
                <D:displayname>Apple Prefix Calendar</D:displayname>
                <cs:getctag>ctag-apple-prefix</cs:getctag>
            </D:prop>
            <D:status>HTTP/1.1 200 OK</D:status>
        </D:propstat>
    </D:response>
    <!-- Calendar using bare calendar element (no namespace prefix) -->
    <D:response>
        <D:href>/dav/cal/admin/bare-calendar/</D:href>
        <D:propstat>
            <D:prop>
                <D:resourcetype xmlns:cal="urn:ietf:params:xml:ns:caldav">
                    <D:collection/>
                    <calendar xmlns="urn:ietf:params:xml:ns:caldav"/>
                </D:resourcetype>
                <D:displayname>Bare Calendar Element</D:displayname>
                <cs:getctag>ctag-bare-element</cs:getctag>
            </D:prop>
            <D:status>HTTP/1.1 200 OK</D:status>
        </D:propstat>
    </D:response>
</D:multistatus>
