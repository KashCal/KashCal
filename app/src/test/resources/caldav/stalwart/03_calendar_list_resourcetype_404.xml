<?xml version="1.0" encoding="UTF-8"?>
<!-- Edge case: resourcetype in 404 propstat should NOT be detected as calendar -->
<!-- This can happen when querying a non-existent collection or permission denied -->
<D:multistatus xmlns:D="DAV:" xmlns:A="http://apple.com/ns/ical/" xmlns:cs="http://calendarserver.org/ns/" xmlns:C="urn:ietf:params:xml:ns:caldav">
    <D:response>
        <D:href>/dav/cal/admin/nonexistent-calendar/</D:href>
        <!-- Resourcetype in 404 propstat - this is NOT a valid calendar -->
        <D:propstat>
            <D:prop>
                <D:resourcetype>
                    <D:collection/>
                    <C:calendar/>
                </D:resourcetype>
                <D:displayname>Ghost Calendar</D:displayname>
            </D:prop>
            <D:status>HTTP/1.1 404 Not Found</D:status>
        </D:propstat>
    </D:response>
</D:multistatus>
